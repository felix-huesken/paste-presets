<?xml version="1.0" encoding="UTF-8"?>
<schemalist  gettext-domain="paste-presets">
  <schema id="org.gnome.shell.extensions.paste-presets"
          path="/org/gnome/shell/extensions/paste-presets/">
    <key name="shortcutopen" type="as">
      <default><![CDATA[['<Super>c']]]></default>
      <summary>Open</summary>
      <description>Keyboard shortcut to open the extension menu and select the next entry. Press repeatedly to select the next entry.</description>
    </key>
    <key name="path" type="s">
      <default>""</default>
      <summary>extension path</summary>
    </key>
    <key name="indicator" type="b">
      <default>true</default>
      <summary>show indicator</summary>
    </key>
  </schema>
</schemalist>

